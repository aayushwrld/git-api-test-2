<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="1" author="johnDoe">This is a test data, this shoudl appear instead of code.</changeSet>

    <changeSet id="2" author="johnDoe">
        <update tableName="user">
            <column name="name" value="John Doe"/>
            <where>id = 1</where>
        </update>
    </changeSet>

    <changeSet id="3" author="johnDoe">
        <insert tableName="user">
            <column name="id" value="1"/>
            <column name="name" value="John Doe"/>
        </insert>
    </changeSet>

    <changeSet id="4" author="johnDoe">
        <comment>This change set will be updated</comment>
        <change>
            <value>Some initial content</value>
        </change>
    </changeSet>

</databaseChangeLog>